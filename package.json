{
    "name": "contacts-api",
    "version": "1.0.0",
    "description": "API that server contact info",
    "type": "module",
    "private": "true",
    "scripts": {
        "start": "node ./src/index.js",
        "dev": "nodemon ./src/index.js"
    },
    "keywords": [
        "API",
        "REST",
        "contacts"
    ],
    "author": "Luis Machaca(luismacode)",
    "license": "Apache-2.0",
    "devDependencies": {
        "eslint": "8.0.1",
        "eslint-config-standard": "17.1.0",
        "eslint-plugin-import": "2.25.2",
        "eslint-plugin-n": "15.0.0 || 16.0.0 ",
        "eslint-plugin-promise": "6.0.0",
        "nodemon": "3.0.1",
        "prettier": "3.0.0"
    },
    "dependencies": {
        "@sinclair/typebox": "0.30.4",
        "ajv": "8.12.0",
        "ajv-errors": "3.0.0",
        "ajv-formats": "2.1.1",
        "dotenv": "16.3.1",
        "express": "4.18.2",
        "mongoose": "7.3.4",
        "swagger-jsdoc": "6.2.8",
        "swagger-ui-express": "5.0.0",
        "uuid": "9.0.0"
    },
    "imports": {
        "#Config/*": "./src/config/*",
        "#Controllers/*": "./src/controllers/*",
        "#RoutesV1/*": "./src/v1/routes/*",
        "#Schemas/*": "./src/schemas/*",
        "#Services/*": "./src/services/*",
        "#Database/*": "./src/database/*",
        "#DTO/*": "./src/dto/*"
    }
}
